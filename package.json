{
  "name": "@didomi/react",
  "version": "1.8.1",
  "description": "didomi-react-test React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "nwb build-react-component && cp -r ./index.d.ts  ./lib/index.d.ts",
    "clean": "nwb clean-module && nwb clean-demo",
    "prepublishOnly": "npm run build",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "lint": "prettier --check src/ tests/",
    "lint:fix": "prettier --write src/ tests/"
  },
  "dependencies": {
    "dset": "^2.0.1"
  },
  "peerDependencies": {
    "react": "^16.8.0 || ^17.0.0 || ^18.0.0"
  },
  "devDependencies": {
    "envsub": "^3.1.0",
    "nwb": "0.23.x",
    "prettier": "2.1.2",
    "react": "^16.8.0",
    "react-dom": "^16.8.0"
  },
  "author": "Didomi",
  "homepage": "https://www.didomi.io/",
  "license": "MIT",
  "keywords": [
    "didomi",
    "sdk",
    "gdpr",
    "react"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/didomi/react"
  }
}
